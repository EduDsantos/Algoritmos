algoritmo "Contador"
var
     contador, valor, salto:inteiro
inicio
      contador <- 0
      escreval("contar ate ")
      leia(valor)
      escreval("em quantas casas ? ")
      leia(salto)
      enquanto contador <= valor faca
               escreval(contador)
               contador <- contador + salto
      fimenquanto
fimalgoritmo