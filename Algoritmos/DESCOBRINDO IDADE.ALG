algoritmo "Descobrindo idade"
var
   idade, ano_nasc, ano_atual:inteiro
inicio
      escreval("em que ano estamos? ")

      leia(ano_atual)

      escreval("em que ano eu nasci? ")

      leia(ano_nasc)

      idade<- ano_atual - ano_nasc

      escreval("entao eu tenho ", idade, " anos")

fimalgoritmo