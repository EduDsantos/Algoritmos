algoritmo "FazSomaProcedimento"
var
   x, y:inteiro
   procedimento soma(a , b:inteiro)
   inicio
         escreval("recebi o valor de " ,a)
         escreval("recebi o valor de " ,b)
         escreval("a soma dos valores sao ", a + b)
   fimprocedimento
inicio
        x<- 5
        y<- 3
        soma(x,y)
fimalgoritmo